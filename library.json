{
  "name": "MarlinSimulator",
  "keywords": "MarlinSimulator",
  "description": "Marlin Simulator UI",
  "repository":
  {
    "type": "git",
    "url": "https://github.com/p3p/MarlinSimulator.git"
  },
  "version": "0.1.0",
  "authors":
  [
    {
        "url": null,
        "maintainer": true,
        "email": "p3p@p3psoft.co.uk",
        "name": "Chris Pepper"
    }
  ],
  "build": {
    "libLDFMode": "deep",
    "srcDir": "",
    "includeDir": "",
    "srcFilter": [
      "+<src/**/*.cpp>",
      "+<vendor/imgui-1.92.3-docking/*.cpp>",
      "+<vendor/imgui-1.92.3-docking/backends/*.cpp>",
      "+<vendor/implot-3da8bd3/*.cpp>",
      "+<vendor/ImGuiFileDialog-0.6.7/*.cpp>",
      "+<vendor/vcd-writer-b4fe347/src/*.cpp>",
      "+<vendor/glad/glad.c>"
    ],
    "flags": [
      "-Isrc",
      "-Iinclude",
      "-Ivendor/imgui-1.92.3-docking",
      "-Ivendor/imgui-1.92.3-docking/backends",
      "-Ivendor/implot-3da8bd3",
      "-Ivendor/ImGuiFileDialog-0.6.7",
      "-Ivendor/vcd-writer-b4fe347/include",
      "-Ivendor/glad/include",
      "-DIMGUI_IMPL_OPENGL_LOADER_GLAD"
    ]
  },
  "frameworks": "*",
  "platforms": "native"
}
